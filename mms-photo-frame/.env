# description: Your Twilio phone number for sending and receiving messages
# format: phone_number
# required: true
TWILIO_PHONE_NUMBER=

# description: Relative path to the Twilio Function that adds items to the Sync List
# configurable: false
POST_HANDLER_PATH=/post-to-frame

# description: Name of the Sync List used to store post data
# configurable: false
SYNC_LIST_NAME=MMS_SYNC_LIST